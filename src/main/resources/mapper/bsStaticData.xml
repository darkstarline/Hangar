<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bsStaticData">
    <sql id="bsStaticDataList"> code_type,code_value,code_name,code_desc,code_type_alias,state,extern_code_type</sql>
    <select id="getBeans"   parameterType="BsStaticData" resultType="BsStaticData">
        SELECT <include refid="bsStaticDataList"/>
        FROM bs_static_data
        <where>
            <if test="codeType!=null and codeType!=''">
                and code_type = #{codeType}
            </if>
            <if test="codeValue!=null and codeValue!=''">
                and code_value = #{codeValue}
            </if>
            <if test="codeName!=null and codeName!=''">
                and code_name = #{codeName}
            </if>
            <if test="codeDesc!=null and codeDesc!=''">
                and code_desc = #{codeDesc}
            </if>
            <if test="codeTypeAlias!=null and codeTypeAlias!=''">
                and code_type_alias = #{codeTypeAlias}
            </if>
            <if test="state!=null and state!=''">
                and state = #{state}
            </if>
            <if test="externCodeType!=null and externCodeType!=''">
                and extern_code_type = #{externCodeType}
            </if>
        </where>
    </select>
</mapper>